@model IEnumerable<clouctech_wbl.Models.tb_user>

@{
    ViewBag.Title = "Index";
}

<h2>User Lists</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.u_pwd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.u_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.u_contact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.u_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.u_email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.u_pwd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.u_email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.u_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.u_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.u_id })
        </td>
    </tr>
}

</table>
